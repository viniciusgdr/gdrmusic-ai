{
  "compilerOptions": {
    "outDir": "dist",
    "module": "commonjs",
    "target": "es2019",
    "esModuleInterop": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "strict": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
